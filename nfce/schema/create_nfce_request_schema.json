{
  "definitions": {
    "cnpj": {
      "type": "string",
      "title": "CNPJ Schema",
      "description": "CNPJ de pessoa jurídica",
      "examples": [
        "81043396000156"
      ],
      "pattern": "^([0-9]{14})$"
    },
    "endereco": {
      "type": "object",
      "title": "Endereco Schema",
      "description": "Endereço (do cliente ou de entrega)",
      "required": [
        "logradouro",
        "numero",
        "bairro",
        "codigo_municipio",
        "nome_municipio",
        "cep",
        "uf"
      ],
      "properties": {
        "logradouro": {
          "type": "string",
          "title": "Logradouro Schema",
          "default": "",
          "examples": [
            "Rua do Analista de Sistemas"
          ],
          "minLength": 2,
          "maxLength": 60
        },
        "numero": {
          "type": "string",
          "title": "Numero Schema",
          "default": "",
          "examples": [
            "256"
          ],
          "minLength": 1,
          "maxLength": 60
        },
        "complemento": {
          "type": "string",
          "title": "Complemento Schema",
          "default": "",
          "examples": [
            "Sala 128"
          ],
          "minLength": 1,
          "maxLength": 60
        },
        "bairro": {
          "type": "string",
          "title": "Bairro Schema",
          "default": "",
          "examples": [
            "Planalto"
          ],
          "minLength": 2,
          "maxLength": 60
        },
        "codigo_municipio": {
          "type": "integer",
          "title": "codigo_municipio Schema",
          "description": "Código do município de acordo com a Tabela de Municípios do IBGE; informar 9999999 para exterior",
          "examples": [
            3304557,
            9999999
          ]
        },
        "nome_municipio": {
          "type": "string",
          "title": "Municipio Schema",
          "default": "",
          "examples": [
            "Uberlândia"
          ],
          "minLength": 2,
          "maxLength": 60
        },
        "cep": {
          "type": "string",
          "title": "Cep Schema",
          "description": "Código de Endereçamento Postal brasileiro",
          "examples": [
            "38413162"
          ],
          "pattern": "^([0-9]{8})$"
        },
        "uf": {
          "$ref": "#/definitions/uf"
        },
        "codigo_pais": {
          "type": "integer",
          "title": "Codigo_pais Schema",
          "description": "Código do país de acordo com a Tabela de Código de País do BACEN",
          "examples": [
            1058,
            2453
          ]
        },
        "nome_pais": {
          "type": "string",
          "title": "Nome_pais Schema",
          "default": "",
          "examples": [
            "Brasil"
          ],
          "minLength": 1,
          "maxLength": 60
        },
        "telefone": {
          "type": "string",
          "title": "Telefone Schema",
          "default": "",
          "examples": [
            "34987654321"
          ],
          "pattern": "^([0-9]{6,14})$"
        },
        "cpf_cnpj": {
          "type": "string",
          "title": "Documento Schema",
          "description": "CPF para pessoa física, CNPJ para pessoa jurídica ou número do documento legal para estrangeiro (passaporte, por exemplo)",
          "examples": [
            "02558352482",
            "81043396000156",
            "CVV006104"
          ],
          "pattern": "^([0-9]{11,14})|([0-9A-Z]{5,20})$"
        }
      }
    },
    "cpf_cnpj": {
      "type": "string",
      "title": "Documento Schema",
      "description": "CPF para pessoa física, CNPJ para pessoa jurídica ou número do documento legal para estrangeiro (passaporte, por exemplo)",
      "examples": [
        "02558352482",
        "81043396000156",
        "CVV006104"
      ],
      "pattern": "^([0-9]{11,14})|([0-9A-Z]{5,20})$"
    },
    "uf": {
      "type": "string",
      "title": "Unidade_federativa Schema",
      "description": "Sigla da Unidade Federativa brasileira; utilizar 'EX' para endereços internacionais",
      "examples": [
        "MG",
        "DF",
        "BA",
        "SP",
        "EX"
      ],
      "minLength": 2,
      "maxLength": 2
    }
  },
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://emites-client.nexaas.com/nfce.json",
  "type": "object",
  "title": "Root Schema",
  "required": [
    "destino_operacao",
    "finalidade_nfe",
    "forma_pagamento",
    "indicador_presenca",
    "natureza_operacao",
    "produtos",
    "tipo_impressao",
    "tipo_operacao",
    "transporte"
  ],
  "properties": {
    "cliente": {
      "$id": "#/properties/cliente",
      "type": "object",
      "title": "Cliente Schema",
      "required": [
        "cpf_cnpj",
        "nome"
      ],
      "properties": {
        "cpf_cnpj": {
          "$id": "#/properties/cliente/properties/cpf_cnpj",
          "$ref": "#/definitions/cpf_cnpj"
        },
        "email": {
          "$id": "#/properties/cliente/properties/email",
          "type": "string",
          "title": "Email Schema",
          "examples": [
            "luiz.buiatte@nexaas.com"
          ],
          "format": "email",
          "minLength": 1,
          "maxLength": 60
        },
        "endereco": {
          "$id": "#/properties/cliente/properties/endereco",
          "$ref": "#/definitions/endereco"
        },
        "indicador_inscricao_estadual": {
          "$id": "#/properties/cliente/properties/indicador_inscricao_estadual",
          "type": "integer",
          "title": "Indicar de inscrição estadual Schema",
          "description": "Tipo de inscrição estadual do cliente",
          "default": 9,
          "$comment": "1 = contribuinte, 2 = isento, 9 = nao contribuinte",
          "enum": [ 1, 2, 9 ]
        },
        "inscricao_estadual": {
          "$id": "#/properties/cliente/properties/inscricao_estadual",
          "type": "string",
          "title": "Inscricao Estadual Schema",
          "description": "Inscrição Estadual do cliente (informar apenas se indicador_inscricao_estadual for igual a 1)",
          "examples": [
            "16324302"
          ],
          "pattern": "^([0-9]{2,14})$"
        },
        "nome": {
          "$id": "#/properties/cliente/properties/nome",
          "type": "string",
          "title": "Nome Schema",
          "description": "Utilizar 'NF-E EMITIDA EM AMBIENTE DE HOMOLOGACAO - SEM VALOR FISCAL' para emissão em homologação",
          "examples": [
            "NF-E EMITIDA EM AMBIENTE DE HOMOLOGACAO - SEM VALOR FISCAL"
          ],
          "minLength": 2,
          "maxLength": 60
        },
        "pessoa_fisica_juridica": {
          "$id": "#/properties/cliente/properties/pessoa_fisica_juridica",
          "type": "string",
          "title": "Tipo Pessoa Schema",
          "description": "Classificação da pessoa",
          "default": "person",
          "enum": [
            "person",
            "company",
            "foreign"
          ]
        }
      }
    },
    "destino_operacao": {
      "$id": "#/properties/destino_operacao",
      "type": "integer",
      "title": "Destino_operacao Schema",
      "description": "Local de destino da operação",
      "default": 1,
      "$comment": "1 = interno, 2 = interestadual, 3 = externo",
      "enum": [ 1, 2, 3 ]
    },
    "finalidade_nfe": {
      "$id": "#/properties/finalidade_nfe",
      "type": "integer",
      "title": "Finalidade Schema",
      "description": "Finalidade da emissão da nota fiscal",
      "default": 1,
      "$comment": "1 = normal, 2 = complementar, 3 = ajuste, 4 = devolução",
      "enum": [ 1, 2, 3, 4 ]
    },
    "forma_pagamento": {
      "$id": "#/properties/forma_pagamento",
      "type": "array",
      "minItems": 1,
      "maxItems": 100,
      "title": "Forma_pagamento Schema",
      "items": {
        "$id": "#/properties/forma_pagamento/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "tipo_de_pagamento",
          "valor_do_pagamento"
        ],
        "properties": {
          "tipo_de_pagamento": {
            "$id": "#/properties/forma_pagamento/items/properties/tipo_de_pagamento",
            "type": "string",
            "title": "Tipo Schema",
            "default": "01",
            "$comment": "ver https://myfreecomm.github.io/emites-api-docs/#forma_de_pagamento-xml-pag",
            "enum": [ "01", "02", "03", "04", "05", "10", "11", "12", "13", "14", "15", "90", "99" ]
          },
          "modalidade": {
            "$id": "#/properties/forma_pagamento/items/properties/modalidade",
            "type": "integer",
            "title": "modalidade Schema",
            "default": 0,
            "$comment": "0 = imediato, 1 = prazo",
            "enum": [ 0, 1 ]
          },
          "valor_do_pagamento": {
            "$id": "#/properties/forma_pagamento/items/properties/valor_do_pagamento",
            "type": "number",
            "title": "Valor Schema",
            "default": 0,
            "examples": [ 2.00, 3.5, 100 ]
          },
          "cartao": {
            "$id": "#/properties/forma_pagamento/items/properties/cartao",
            "type": "object",
            "title": "Cartao Schema",
            "description": "Informar apenas se o campo 'tipo' especificar cartão de crédito ou débito",
            "required": [
              "tipo_integracao",
              "numero_autorizacao_operacao"
            ],
            "properties": {
              "tipo_integracao": {
                "$id": "#/properties/forma_pagamento/items/properties/cartao/properties/tipo_integracao",
                "type": "integer",
                "title": "Tipo_integracao Schema",
                "description": "Tipo de integração do processo de pagamento com o sistema de automação da empresa (por exemplo, TEF)",
                "default": 1,
                "$comment": "1 = integrado, 2 = não integrado",
                "enum": [ 1, 2 ]
              },
              "cnpj_credenciadora": {
                "$id": "#/properties/forma_pagamento/items/properties/cartao/properties/cnpj_credenciadora",
                "description": "CNPJ da credenciadora de cartão de crédito ou débito",
                "$ref": "#/definitions/cnpj"
              },
              "bandeira_operadora": {
                "$id": "#/properties/forma_pagamento/items/properties/cartao/properties/bandeira_operadora",
                "type": "string",
                "title": "bandeira_operadora Schema",
                "$comment": "ver https://myfreecomm.github.io/emites-api-docs/#cartao-xml-card",
                "enum": [ "01", "02", "03", "04", "05", "06", "07", "08", "09", "99" ]
              },
              "numero_autorizacao_operacao": {
                "$id": "#/properties/forma_pagamento/items/properties/cartao/properties/numero_autorizacao_operacao",
                "type": "string",
                "title": "Numero_autorizacao Schema",
                "description": "Identifica o número da autorização da transação da operação com cartão de crédito ou débito",
                "examples": [
                  "1234567890"
                ],
                "minLength": 1,
                "maxLength": 20
              }
            }
          }
        }
      }
    },
    "indicador_presenca": {
      "$id": "#/properties/indicador_presenca",
      "type": "integer",
      "title": "Indicador_presenca Schema",
      "description": "Presença do comprador no estabelecimento comercial no momento da operação",
      "default": "presencial",
      "$comment": "0 = nao aplicavel, 1 = presencial, 2 = internet, 3 = telemarketing, 4 = domicilio, 5 = presencial externo, 9 = outros",
      "enum": [ 0, 1, 2, 3, 4, 5, 9 ]
    },
    "informacoes_adicionais": {
      "$id": "#/properties/informacoes_adicionais",
      "type": "object",
      "title": "Informacoes_adicionais Schema",
      "properties": {
        "informacoes_contribuinte": {
          "$id": "#/properties/informacoes_adicionais/properties/informacoes_contribuinte",
          "type": "string",
          "title": "Informacoes_contribuinte Schema",
          "description": "Informações complementares de interesse do contribuinte",
          "examples": [
            "Empresa optante pelo Simples Nacional"
          ],
          "minLength": 1,
          "maxLength": 2000
        },
        "informacoes_fisco": {
          "$id": "#/properties/informacoes_adicionais/properties/informacoes_fisco",
          "type": "string",
          "title": "Informacoes_fisco Schema",
          "description": "Informações adicionais de interesse do Fisco",
          "examples": [
            "Documento Emitido por EPP optante pelo Simples Nacional. Nao gera direito a credito Fiscal de ISS e IPI"
          ],
          "minLength": 1,
          "maxLength": 2000
        },
        "observacoes_fisco": {
          "$id": "#/properties/informacoes_adicionais/properties/observacoes_fisco",
          "type": "array",
          "minItems": 0,
          "maxItems": 10,
          "title": "Observacoes_fisco Schema",
          "items": {
            "$id": "#/properties/informacoes_adicionais/properties/observacoes_fisco/items",
            "type": "object",
            "title": "Items Schema",
            "required": [
              "campo",
              "texto"
            ],
            "properties": {
              "campo": {
                "$id": "#/properties/informacoes_adicionais/properties/observacoes_fisco/items/properties/campo",
                "type": "string",
                "title": "Campo Schema",
                "description": "Identificação do campo",
                "examples": [
                  "Lei"
                ],
                "minLength": 1,
                "maxLength": 20
              },
              "texto": {
                "$id": "#/properties/informacoes_adicionais/properties/observacoes_fisco/items/properties/texto",
                "type": "string",
                "title": "Texto Schema",
                "description": "Conteúdo do campo",
                "examples": [
                  "Observação do campo"
                ],
                "minLength": 1,
                "maxLength": 60
              }
            }
          }
        }
      }
    },
    "lista_autorizacao": {
      "$id": "#/properties/lista_autorizacao",
      "type": "array",
      "minItems": 0,
      "maxItems": 10,
      "title": "Lista_autorizacao Schema",
      "items": {
        "$id": "#/properties/lista_autorizacao/items",
        "type": "string",
        "title": "Items Schema",
        "description": "CPF ou CNPJ autorizado a obter o XML",
        "examples": [
          "77184642538",
          "31065377000152"
        ],
        "minLength": 11,
        "maxLength": 14,
        "pattern": "^([0-9]{11,14})$"
      }
    },
    "natureza_operacao": {
      "$id": "#/properties/natureza_operacao",
      "type": "string",
      "title": "Natureza_operacao Schema",
      "description": "Informar a natureza da operação de que decorrer a saída ou a entrada, tais como: venda, compra, transferência, devolução",
      "examples": [
        "venda",
        "compra",
        "transferência",
        "devolução",
        "importação",
        "remessa (para fins de demonstração)"
      ],
      "minLength": 1,
      "maxLength": 60
    },
    "produtos": {
      "$id": "#/properties/produtos",
      "type": "array",
      "minItems": 1,
      "maxItems": 500,
      "title": "Produtos Schema",
      "items": {
        "$id": "#/properties/produtos/items",
        "type": "object",
        "title": "Items Schema",
        "required": [
          "ncm",
          "codigo_produto",
          "descricao",
          "codigo_origem_produto",
          "quantidade_comercial",
          "quantidade_tributavel",
          "unidade_comercial",
          "unidade_tributavel",
          "valor_total_produto",
          "valor_unitario_comercial",
          "valor_unitario_tributario"
        ],
        "properties": {
          "cest": {
            "$id": "#/properties/produtos/items/properties/cest",
            "type": "string",
            "title": "Codigo_cest Schema",
            "description": "Código Especificador da Substituição Tributária - CEST",
            "examples": [
              "1300402"
            ],
            "minLength": 7,
            "maxLength": 7
          },
          "cfop": {
            "$id": "#/properties/produtos/items/properties/cfop",
            "type": "string",
            "description": "Código Fiscal de Operações e Prestações, identifica a natureza de circulação de uma mercadoria ou a prestação de serviço de transportes",
            "title": "Codigo_cfop Schema",
            "examples": [
              "5103"
            ],
            "minLength": 4,
            "maxLength": 4
          },
          "codigo_ean": {
            "$id": "#/properties/produtos/items/properties/codigo_ean",
            "type": "string",
            "title": "Codigo_ean Schema",
            "description": "GTIN (Global Trade Item Number) do produto, antigo código EAN ou código de barras",
            "examples": [
              "01234567",
              "012345678905",
              "7891000315507",
              "00012345600012"
            ],
            "minLength": 8,
            "maxLength": 14
          },
          "ean_unidade_trib": {
            "$id": "#/properties/produtos/items/properties/codigo_ean",
            "type": "string",
            "title": "ean_unidade_trib Schema",
            "description": "GTIN (Global Trade Item Number) da unidade tributável, antigo código EAN ou código de barras",
            "examples": [
              "01234567",
              "012345678905",
              "7891000315507",
              "00012345600012"
            ],
            "minLength": 8,
            "maxLength": 14
          },
          "extipi": {
            "$id": "#/properties/produtos/items/properties/extipi",
            "type": "string",
            "title": "Codigo_excecao_tipi Schema",
            "description": "Preencher de acordo com o código de exceção da TIPI (Tabela de Incidência do Imposto sobre Produtos Industrializados)",
            "examples": [
              "22"
            ],
            "minLength": 2,
            "maxLength": 3
          },
          "ncm": {
            "$id": "#/properties/produtos/items/properties/ncm",
            "type": "string",
            "title": "Codigo_ncm Schema",
            "description": "Código NCM (Nomenclatura Comum do Mercosul) com 8 dígitos ou 2 dígitos ('00' se não aplicável)",
            "examples": [
              "00",
              "22086000"
            ],
            "minLength": 2,
            "maxLength": 8
          },
          "codigo_produto": {
            "$id": "#/properties/produtos/items/properties/codigo_produto",
            "type": "string",
            "title": "Codigo_produto Schema",
            "description": "Código do produto ou serviço do contribuinte, ou CFOP se não possuir codificação própria (formato CFOP9999)",
            "examples": [
              "14600",
              "CFOP1234"
            ],
            "minLength": 1,
            "maxLength": 60
          },
          "descricao": {
            "$id": "#/properties/produtos/items/properties/descricao",
            "type": "string",
            "title": "Descricao Schema",
            "description": "Descrição do produto ou serviço (utilizar o valor do exemplo para ambiente de homologação)",
            "examples": [
              "NOTA FISCAL EMITIDA EM AMBIENTE DE HOMOLOGACAO - SEM VALOR FISCAL"
            ],
            "minLength": 1,
            "maxLength": 120
          },
          "informacoes_adicionais": {
            "$id": "#/properties/produtos/items/properties/informacoes_adicionais",
            "type": "string",
            "title": "Informacoes_adicionais Schema",
            "description": "Informações adicionais do produto (por exemplo, motivo da devolução)",
            "examples": [
              "Produto devolvido por defeito de fabricação"
            ],
            "minLength": 0,
            "maxLength": 120
          },
          "num_item_pedido": {
            "$id": "#/properties/produtos/items/properties/num_item_pedido",
            "type": "integer",
            "title": "Numero_item_pedido Schema",
            "description": "Item do pedido de compra",
            "examples": [
              1,
              10,
              100
            ]
          },
          "num_pedido": {
            "$id": "#/properties/produtos/items/properties/num_pedido",
            "type": "string",
            "title": "Numero_pedido Schema",
            "description": "Número do pedido de compra, se houver",
            "examples": [
              "5100003910"
            ],
            "minLength": 0,
            "maxLength": 15
          },
          "codigo_origem_produto": {
            "$id": "#/properties/produtos/items/properties/codigo_origem_produto",
            "type": "integer",
            "title": "Origem Schema",
            "description": "Origem do produto (tal como nacional, estrangeiro ou nacional com conteúdo importado)",
            "default": 0,
            "$comment": "ver https://myfreecomm.github.io/emites-api-docs/#tributa-o-xml-imposto, campo 'codigo_origem_produto'",
            "enum": [ 0, 1, 2, 3, 4, 5, 6, 7, 8 ]
          },
          "quantidade_comercial": {
            "$id": "#/properties/produtos/items/properties/quantidade_comercial",
            "type": "number",
            "title": "Quantidade_comercial Schema",
            "description": "Informar a quantidade de comercialização do produto",
            "default": 0,
            "examples": [
              2.00,
              3.5,
              100
            ]
          },
          "quantidade_tributavel": {
            "$id": "#/properties/produtos/items/properties/quantidade_tributaria",
            "type": "number",
            "title": "Quantidade_tributaria Schema",
            "default": 0,
            "description": "Informar a quantidade tributável do produto",
            "examples": [
              2.00,
              3.5,
              100
            ]
          },
          "unidade_comercial": {
            "$id": "#/properties/produtos/items/properties/unidade_comercial",
            "type": "string",
            "title": "Unidade_comercial Schema",
            "description": "Informar a unidade de comercialização do produto",
            "examples": [
              "UN"
            ],
            "minLength": 1,
            "maxLength": 6
          },
          "unidade_tributavel": {
            "$id": "#/properties/produtos/items/properties/unidade_tributaria",
            "type": "string",
            "title": "Unidade_tributaria Schema",
            "description": "Unidade tributável do produto",
            "examples": [
              "UN"
            ],
            "minLength": 1,
            "maxLength": 6
          },
          "valor_desconto": {
            "$id": "#/properties/produtos/items/properties/valor_desconto",
            "type": "number",
            "title": "Valor_desconto Schema",
            "default": 0,
            "examples": [
              2.00,
              3.5,
              100
            ]
          },
          "valor_frete": {
            "$id": "#/properties/produtos/items/properties/valor_frete",
            "type": "number",
            "title": "Valor_frete Schema",
            "default": 0,
            "examples": [
              2.00,
              3.5,
              100
            ]
          },
          "outras_despesas": {
            "$id": "#/properties/produtos/items/properties/outras_despesas",
            "type": "number",
            "title": "Valor_outras_despesas Schema",
            "default": 0,
            "examples": [
              2.00,
              3.5,
              100
            ]
          },
          "valor_seguro": {
            "$id": "#/properties/produtos/items/properties/valor_seguro",
            "type": "number",
